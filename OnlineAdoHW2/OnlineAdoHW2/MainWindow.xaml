        <TextBlock HorizontalAlignment="Left" Margin="41,52,0,0" TextWrapping="Wrap" Text="Enter Logi" VerticalAlignment="Top"/>
        <TextBox x:Name="userLogin" HorizontalAlignment="Left" Height="26" Margin="193,52,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="146"/>
